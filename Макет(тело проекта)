# Unlock-Code
Unlock Code üîê  –ì–µ–Ω–µ—Ä–∞—Ç–æ—Ä –ø–∞—Ä–æ–ª–µ–π –Ω–∞ Python  –í–æ–∑–º–æ–∂–Ω–æ—Å—Ç–∏:  –°–æ–∑–¥–∞–Ω–∏–µ –ø–∞—Ä–æ–ª–µ–π  –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –ø–æ–¥ —Å–µ–±—è  –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ —à–∞–±–ª–æ–Ω—É  –ü–æ–ª—å–∑–∞:  –†–µ—à–∞–µ—Ç –ø—Ä–æ–±–ª–µ–º—É –ø–∞—Ä–æ–ª–µ–π  –ü—Ä–∞–∫—Ç–∏–∫–∞ –∞–ª–≥–æ—Ä–∏—Ç–º–æ–≤  –ü—Ä–æ—Å—Ç–æ. –ü—Ä–∞–∫—Ç–∏—á–Ω–æ. üéØ
Code:
import random
import string
import itertools

class AdvancedPasswordGenerator:
    def __init__(self):
        self.lowercase = string.ascii_lowercase
        self.uppercase = string.ascii_uppercase
        self.digits = string.digits
        self.symbols = "!@#$%^&*()_+-=[]{}|;:,.<>?"
    
    def mode1_random_password(self):
        
        print("\n=== –†–ï–ñ–ò–ú 1: –°–ª—É—á–∞–π–Ω—ã–µ –ø–∞—Ä–æ–ª–∏ ===")
        print("1 - –ü—Ä–æ—Å—Ç–æ–π –ø–∞—Ä–æ–ª—å (—Ç–æ–ª—å–∫–æ –±—É–∫–≤—ã)")
        print("2 - –°–ª–æ–∂–Ω—ã–π –ø–∞—Ä–æ–ª—å (–±—É–∫–≤—ã, —Ü–∏—Ñ—Ä—ã, —Å–∏–º–≤–æ–ª—ã)")
        
        choice = input("–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –ø–∞—Ä–æ–ª—è (1 –∏–ª–∏ 2): ")
        
        if choice == "1":
            
            length = 10
            characters = self.lowercase + self.uppercase
            password = ''.join(random.choice(characters) for _ in range(length))
            print(f"üîπ –ü—Ä–æ—Å—Ç–æ–π –ø–∞—Ä–æ–ª—å: {password}")
            
        elif choice == "2":
         
            length = 16
            characters = self.lowercase + self.uppercase + self.digits + self.symbols
            password = ''.join(random.choice(characters) for _ in range(length))
            print(f"üî∏ –°–ª–æ–∂–Ω—ã–π –ø–∞—Ä–æ–ª—å: {password}")
        else:
            print("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä!")
    
    def mode2_custom_symbols(self):
    
        print("\n=== –†–ï–ñ–ò–ú 2: –°–≤–æ–∏ —Å–∏–º–≤–æ–ª—ã ===")
        
        # –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –≤–≤–æ–¥–∏—Ç —Ä–∞–∑—Ä–µ—à–µ–Ω–Ω—ã–µ —Å–∏–º–≤–æ–ª—ã
        allowed_chars = input("–í–≤–µ–¥–∏—Ç–µ —Ä–∞–∑—Ä–µ—à–µ–Ω–Ω—ã–µ —Å–∏–º–≤–æ–ª—ã (–Ω–∞–ø—Ä–∏–º–µ—Ä: abc123!@): ")
        
        if not allowed_chars:
            print("‚ùå –ù—É–∂–Ω–æ –≤–≤–µ—Å—Ç–∏ —Ö–æ—Ç—è –±—ã –æ–¥–∏–Ω —Å–∏–º–≤–æ–ª!")
            return
        
        print("1 - –ü—Ä–æ—Å—Ç–æ–π –ø–∞—Ä–æ–ª—å (8 —Å–∏–º–≤–æ–ª–æ–≤)")
        print("2 - –°–ª–æ–∂–Ω—ã–π –ø–∞—Ä–æ–ª—å (12 —Å–∏–º–≤–æ–ª–æ–≤)")
        choice = input("–í—ã–±–µ—Ä–∏—Ç–µ –¥–ª–∏–Ω—É: ")
        
        if choice == "1":
            length = 8
        elif choice == "2":
            length = 12
        else:
            print("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä!")
            return
        

        password = ''.join(random.choice(allowed_chars) for _ in range(length))
        print(f"üîπ –í–∞—à –ø–∞—Ä–æ–ª—å: {password}")
    
    def mode3_password_recovery(self):
      
        print("\n=== –†–ï–ñ–ò–ú 3: –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–∞—Ä–æ–ª—è ===")
        
        length = int(input("–í–≤–µ–¥–∏—Ç–µ –¥–ª–∏–Ω—É –ø–∞—Ä–æ–ª—è: "))
        known_parts = input("–í–≤–µ–¥–∏—Ç–µ –∏–∑–≤–µ—Å—Ç–Ω—ã–µ —á–∞—Å—Ç–∏ –ø–∞—Ä–æ–ª—è (–∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ ? –¥–ª—è –Ω–µ–∏–∑–≤–µ—Å—Ç–Ω—ã—Ö —Å–∏–º–≤–æ–ª–æ–≤): ")
        
        if len(known_parts) != length:
            print(f"‚ùå –î–ª–∏–Ω–∞ –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å {length} —Å–∏–º–≤–æ–ª–æ–≤!")
            return
        
    
        print("–ö–∞–∫–∏–µ —Å–∏–º–≤–æ–ª—ã –º–æ–≥—É—Ç –±—ã—Ç—å –≤ –ø–∞—Ä–æ–ª–µ?")
        print("1 - –¢–æ–ª—å–∫–æ —Ü–∏—Ñ—Ä—ã (0-9)")
        print("2 - –¢–æ–ª—å–∫–æ –±—É–∫–≤—ã (a-z, A-Z)")
        print("3 - –ë—É–∫–≤—ã –∏ —Ü–∏—Ñ—Ä—ã")
        print("4 - –í—Å–µ —Å–∏–º–≤–æ–ª—ã")
        
        char_choice = input("–í—ã–±–µ—Ä–∏—Ç–µ –Ω–∞–±–æ—Ä —Å–∏–º–≤–æ–ª–æ–≤ (1-4): ")
        
        if char_choice == "1":
            possible_chars = self.digits
        elif char_choice == "2":
            possible_chars = self.lowercase + self.uppercase
        elif char_choice == "3":
            possible_chars = self.lowercase + self.uppercase + self.digits
        elif char_choice == "4":
            possible_chars = self.lowercase + self.uppercase + self.digits + self.symbols
        else:
            print("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä!")
            return
        
   
        print("\nüîç –í–æ–∑–º–æ–∂–Ω—ã–µ –ø–∞—Ä–æ–ª–∏:")
        found_count = 0
        
        for i in range(1000):  # –û–≥—Ä–∞–Ω–∏—á–∏–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø–æ–ø—ã—Ç–æ–∫
            password = []
            for char in known_parts:
                if char == '?':
                    password.append(random.choice(possible_chars))
                else:
                    password.append(char)
            
            result = ''.join(password)
            print(f"–í–∞—Ä–∏–∞–Ω—Ç {found_count + 1}: {result}")
            found_count += 1
            
            if found_count >= 5:  # –ü–æ–∫–∞–∂–µ–º —Ç–æ–ª—å–∫–æ 5 –≤–∞—Ä–∏–∞–Ω—Ç–æ–≤
                break
        
        if found_count == 0:
            print("‚ùå –ù–µ —É–¥–∞–ª–æ—Å—å –Ω–∞–π—Ç–∏ –ø–æ–¥—Ö–æ–¥—è—â–∏–µ –ø–∞—Ä–æ–ª–∏!")
    
    def show_menu(self):
 
        while True:
            print("\n" + "="*50)
            print("üîê UNLOCK CODE - –ì–µ–Ω–µ—Ä–∞—Ç–æ—Ä –ø–∞—Ä–æ–ª–µ–π")
            print("="*50)
            print("1 - –°–ª—É—á–∞–π–Ω—ã–π –ø–∞—Ä–æ–ª—å (–ø—Ä–æ—Å—Ç–æ–π/—Å–ª–æ–∂–Ω—ã–π)")
            print("2 - –ü–∞—Ä–æ–ª—å –∏–∑ —Å–≤–æ–∏—Ö —Å–∏–º–≤–æ–ª–æ–≤")
            print("3 - –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–∞—Ä–æ–ª—è –ø–æ —à–∞–±–ª–æ–Ω—É")
            print("0 - –í—ã—Ö–æ–¥")
            print("="*50)
            
            choice = input("–í—ã–±–µ—Ä–∏—Ç–µ —Ä–µ–∂–∏–º —Ä–∞–±–æ—Ç—ã (0-3): ")
            
            if choice == "1":
                self.mode1_random_password()
            elif choice == "2":
                self.mode2_custom_symbols()
            elif choice == "3":
                self.mode3_password_recovery()
            elif choice == "0":
                print("üëã –î–æ —Å–≤–∏–¥–∞–Ω–∏—è!")
                break
            else:
                print("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä! –ü–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.")


if __name__ == "__main__":
    generator = AdvancedPasswordGenerator()
    generator.show_menu()
